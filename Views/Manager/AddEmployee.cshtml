@model WebNutFactory.Models.Employee
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Form Example</title>
</head>

<h3>Add Employee</h3>
<br>
<br>
<body>
     
        <form asp-action="SubmitForm5" method="post">
            <label asp-for="FirstName">First Name :</label>
            <input asp-for="FirstName" type="text" />
            <label asp-for="LastName">Last Name  :</label>
            <input asp-for="LastName" type="text" />
            <label asp-for="Salary"> Salary :</label>
            <input asp-for="Salary" type="text" />
            <label asp-for="InventoryID">InventoryID :</label>
            <input asp-for="InventoryID" type="text" />
            <br>
            <br>
            <label asp-for="Email">Email :</label>
            <input asp-for="Email" type="text" />
            <label asp-for="PhoneNumber">PhoneNumber:</label>
            <input asp-for="PhoneNumber" type="text" />
            <br>
            <br>
            <button type="submit">Add Employee</button>
            <br />
            <br />
        <a href="https://localhost:5166/Manager/DisplayEmployees" class="btn btn-primary">Display Employees</a>
            <br>
            <br>
            <p> <span asp-validation-for="FirstName"></span></p>
            <p><span asp-validation-for="LastName"></span></p>
            <p><span asp-validation-for="Salary"></span></p>
            <p><span asp-validation-for="PhoneNumber"></span></p>
            <p><span asp-validation-for="InventoryID"></span></p>
        </form>
</body>



